{
  "schemaVersion": 1,
  "id": "smitemc_hotkey",
  "version": "${version}",
  "name": "SmiteMC Menu Hotkey",
  "description": "Adds a shortcut (hotkey) for opening the main menu for the Minecraft Earth game mode on the SmiteMC Network.",
  "authors": [
    "LeonJS_"
  ],
  "contact": {
    "sources": "https://github.com/SmiteMCde/Menu-Hotkey",
    "issues": "https://github.com/SmiteMCde/Menu-Hotkey/issues"
  },
  "license": "All rights reserved",
  "icon": "assets/smitemc_hotkey/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.smitemc.fabric.HotkeyModFabric"
    ],
    "client": [
      "de.smitemc.fabric.client.HotkeyModFabricClient"
    ]
  },
  "mixins": [
    "smitemc_hotkey.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.5",
    "java": ">=21",
    "architectury": ">=16.1.4"
